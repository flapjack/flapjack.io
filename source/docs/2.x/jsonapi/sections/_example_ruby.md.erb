<%
ext = if resource_linked_type.nil?
  resource_name
else
  "#{resource_type}_link_#{resource_linked_name}"
end
%>

```ruby
require 'flapjack-diner'
Flapjack::Diner.base_uri('localhost:3081')

<% case method_type
   when :post %>
Flapjack::Diner.create_<%= ext -%>(
  :id => '<%= uuid_1 -%>'
)

# or

Flapjack::Diner.create_<%= ext -%>({
  :id => '<%= uuid_1 -%>'
}, {
  :id => '<%= uuid_2 -%>'
})
<% when :get %>
Flapjack::Diner.<%= ext -%>(
  :fields => {},
  :filter => {},
  :include => [],
  :sort => '',
  :page => 1, :per_page => 20
)
<% when :patch %>
Flapjack::Diner.update_<%= ext -%>(
  :id => '<%= uuid_1 -%>'
)

# or

Flapjack::Diner.update_<%= ext -%>({
  :id => '<%= uuid_1 -%>'
}, {
  :id => '<%= uuid_2 -%>'
})
<% when :delete %>
Flapjack::Diner.delete_<%= ext -%>(
  '<%= uuid_1 %>'
)

# or

Flapjack::Diner.delete_<%= ext -%>(
  '<%= uuid_1 %>',
  '<%= uuid_2 %>'
)
<%
end -%>
```
